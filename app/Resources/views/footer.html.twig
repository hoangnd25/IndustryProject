<footer class="toolkit">
    <div class="container">
        <div class="col-md-12">
            <div class="row">
                <div class="col-md-2">
                    {% set footer1 = cms_menu_get('footer-1') %}
                    <h4>{{ footer1.name }}</h4>
                    <ul class="link-list">
                        {% for item in footer1.items %}
                            <li><a class="{{ item.class }}" href="{{ item.url }}">{{ item.name }}</a> </li>
                        {% endfor %}
                    </ul>
                </div>
                <div class="col-md-2">
                    {% set footer2 = cms_menu_get('footer-2') %}
                    <h4>{{ footer2.name }}</h4>
                    <ul class="link-list">
                        {% for item in footer2.items %}
                            <li><a class="{{ item.class }}" href="{{ item.url }}">{{ item.name }}</a> </li>
                        {% endfor %}
                    </ul>
                </div>
                <div class="col-md-2">
                    {% set footer3 = cms_menu_get('footer-3') %}
                    <h4>{{ footer3.name }}</h4>
                    <ul class="link-list">
                        {% for item in footer3.items %}
                            <li><a class="{{ item.class }}" href="{{ item.url }}">{{ item.name }}</a> </li>
                        {% endfor %}
                    </ul>
                </div>
                <div class="col-md-2">
                    <h4></h4>
                    <ul class="link-list"></ul>
                </div>
                <div class="clearfix visible-sm"></div>
                <div class="col-md-3 col-md-offset-1 newsletter">
                    <div class="col-xs-12">
                        {% set footer5 = cms_menu_get('footer-5') %}
                        <h4>{{ footer5.name }}</h4>
                        {% for item in footer5.items %}
                            <a class="{{ item.class }}" href="{{ item.url }}">{{ item.name }}</a>
                        {% endfor %}
                    </div>
                </div>
            </div>
            <div class="row footer-links">
                <div class="col-md-8">
                    {% set footer4 = cms_menu_get('footer-4') %}
                    <span>{{ footer4.name }}</span>
                    <ul class="link-list inline">
                        {% for item in footer4.items %}
                            <li><a class="{{ item.class }}" href="{{ item.url }}">{{ item.name }}</a>{% if not loop.last %}&nbsp;|&nbsp;{% endif %}</li>
                        {% endfor %}
                    </ul>
                </div>
                <div class="col-md-4 text-right">
                    <p>Â© GS1 Australia 2015</p>
                </div>
            </div>
        </div>
    </div>
</footer>